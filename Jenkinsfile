    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'Build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Test'
            }
        }
        stage('Release') {
            steps {
                echo 'Test'
            }
        }
    }
}
